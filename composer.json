{
	"name": "maslosoft/addendum",
	"description": "Easy to use php annotations",
	"keywords": [
		"annotations",
		"annotation",
		"addendum",
		"metadata"
	],
	"license": [
		"AGPL",
		"Commercial"
	],
	"authors": [
		{
			"name": "Peter Maselkowski",
			"email": "pmaselkowski@gmail.com",
			"homepage": "http://maslosoft.com",
			"role": "Developer"
		}
	],
	"minimum-stability": "stable",
	"autoload": {
		"psr-4": {
			"Maslosoft\\Addendum\\": "src"
		}
	},
	"autoload-dev": { 
	    "psr-4": { 
	      "AddendumTest\\": "tests", 
	      "Maslosoft\\AddendumTest\\Models\\": "tests/models", 
	      "Maslosoft\\AddendumTest\\Annotations\\": "tests/annotations" 
	    } 
	  }, 
	"suggest": {
		"monolog/monolog": "Sends your logs to files, sockets, inboxes, databases and various web services"
	},
	"require-dev": {
		"codeception/codeception": "*", 
    	"codeception/specify": "0.4.*" ,
		"maslosoft/hedron": "*"
	},
	"require": {
		"php": ">=5.5.0",
		"psr/log": "~1",
		"maslosoft/embedi": "*",
		"maslosoft/signals": "*",
		"maslosoft/gazebo": "*",
		"maslosoft/cli-shared": "*",
		"maslosoft/cache": "*"
	},
	"homepage": "https://maslosoft.com/addendum/"
}